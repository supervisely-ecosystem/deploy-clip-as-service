{
	"type": "app",
	"version": "2.0.0",
	"name": "CLIP Service",
	"description": "Deploy CLIP (Contrastive Language-Image Pre-Training) model as a microservice for generating high-quality image and text embeddings",
	"entrypoint": "python -m clip_server clip.yml --quiet --log-level WARNING",
	"docker_image": "supervisely/deploy-cas:0.1.0",
	"port": 8000,
	"categories": ["system"],
	"headless": true,
	"icon": "https://github.com/supervisely-ecosystem/embeddings-generator/releases/download/v0.1.0/clip_icon.jpg",
	"icon_cover": true,
	"poster": "https://github.com/supervisely-ecosystem/embeddings-generator/releases/download/v0.1.0/clip_poster.jpg",
	"instance_version": "6.12.44",
	"need_gpu": true,
	"gpu": "preferred",
	"community_agent": false,
	"restart_policy": "on_error",
	"task_location": "application_sessions"
}
